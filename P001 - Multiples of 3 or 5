def multy_nums(n):
"""
This function gets a number and returns the sum of the multiples of 3 or 5 between 1 and (the number -1).
:param n:   (int) a positive integer
:output:    (int) a the sum of the multiples of 3 or 5 between 1 and (the number -1)
"""
    sum = 0
    for i in range(1,n):
        if (i % 3 == 0) or (i % 5 == 0):
            sum += i 
    return sum
    
